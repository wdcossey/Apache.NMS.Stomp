<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Version>1.5.4.3218</Version>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <Authors>Apache Software Foundation, William D Cossey</Authors>
    <Product>Apache NMS for Stomp Class Library for .net standard</Product>
    <Description>Apache NMS for Stomp Class Library (.Net Standard Messaging Library Implementation): An implementation of the NMS API for Stomp</Description>
    <Copyright>Copyright (C) 2005-2013 Apache Software Foundation</Copyright>
    <PackageLicenseUrl></PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/wdcossey/Apache.NMS.Stomp.NetStd</PackageProjectUrl>
    <RepositoryUrl>https://github.com/wdcossey/Apache.NMS.Stomp.NetStd</RepositoryUrl>
    <PackageTags>Apache,NMS,Messaging,Stomp,NetStandard</PackageTags>
    <PackageReleaseNotes>Apache.NMS.Stomp (http://activemq.apache.org/nms/apachenmsstomp.html) for .NET Standard.
Added CLSCompliant assembly info.
Updated Apache.NMS.NetStd dependancy.</PackageReleaseNotes>
    <RootNamespace>Apache.NMS.Stomp</RootNamespace>
    <AssemblyName>Apache.NMS.Stomp</AssemblyName>
    <PackageId>Apache.NMS.Stomp.NetStd</PackageId>
    <AssemblyVersion>1.5.4.3218</AssemblyVersion>
    <PackageLicenseFile>LICENSE.txt</PackageLicenseFile>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <DefineConstants>TRACE;NETCORE</DefineConstants>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DefineConstants>TRACE;DEBUG;NETCORE</DefineConstants>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Apache.NMS.NetStd" Version="1.7.1.3899" />
  </ItemGroup>

  <ItemGroup>
    <None Include="LICENSE.txt">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
